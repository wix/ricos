{
  "name": "wix-rich-content-monorepo",
  "workspaces": [
    "e2e/test-env",
    "examples/main",
    "examples/storybook",
    "packages/!(template-*)/web",
    "bundle-analyzer",
    "!website"
  ],
  "resolutions": {
    "@types/react-i18next/@types/i18next": "11.9.0",
    "ab-translate": "1.1.557",
    "@types/react": "^16.13.1",
    "ts-proto-descriptors": "1.2.1"
  },
  "private": true,
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix/ricos.git"
  },
  "scripts": {
    "clean": "lerna clean --yes && rm -rf node_modules && rm -f package-lock.json && rm -f yarn.lock",
    "reinstall": "yarn clean && npx midgard-yarn install",
    "rebuild": "npm run reinstall && npm run build",
    "postinstall": "touch .env",
    "build": "lerna run --stream --ignore 'wix-rich-content-@(test-env|example|storybook)' build",
    "build:dev": "lerna run --stream --ignore 'wix-rich-content-@(test-env|example|storybook)' build:dev",
    "build:core": "npm run build:dev -- --scope 'ricos-content' && npm run build:dev -- --scope 'wix-rich-content-@(common|editor|viewer)'",
    "prewatch": "lerna run build --scope wix-rich-content-*common* && lerna run build --scope wix-rich-content-editor && lerna run build --scope ricos-common",
    "prebuild": "node scripts/mergeTranslations",
    "watch": "lerna run --parallel --ignore ${LERNA_IGNORE:-'wix-rich-content-@(test-env|example|storybook)'}  watch",
    "watch:core": "LERNA_IGNORE='wix-rich-content-@(plugin-*|fullscreen)' npm run watch",
    "release": "node scripts/publishPackages",
    "deploy": "node scripts/deployment/deployExamples",
    "deployPRComment": "node scripts/deployment/deployPRComment",
    "createVersion": "node scripts/createVersion",
    "preRelease": "node scripts/preRelease",
    "storybook": "npm run start --prefix examples/storybook",
    "exampleApp": "npm run start --prefix examples/main",
    "docs": "npm run start --prefix website",
    "docs:deploy": "npm run deploy --prefix website",
    "saveBundlesSizesBaseline": "npm run build && npm run save --prefix bundle-analyzer",
    "analyzeBundles": "npm run build && npm run analyzeBundles:nobuild --silent",
    "analyzeBundles:nobuild": "npm run analyzeAndCompare --prefix bundle-analyzer --silent",
    "analyzePlugin": "npm run build && npm run analyze --prefix bundle-analyzer",
    "analyzeWrapper": "lerna run build --stream --scope ricos-viewer && npm run analyze --prefix bundle-analyzer ricos-viewer",
    "e2e": "start-server-and-test e2e:app http://localhost:3002 e2e:run",
    "e2e:debug": "concurrently \"npm run e2e:app:dev\" \"npm run e2e:app:wait && npm run e2e:debug:openUI\"",
    "e2e:debug:openUI": "npx cypress open --env MATCH_CONTENT_STATE=true,debug=true",
    "e2e:testContent": "start-server-and-test e2e:app http://localhost:3002 e2e:run:testContent",
    "e2e:updateSnapshots": "start-server-and-test e2e:app http://localhost:3002 e2e:run:update",
    "e2e:run": "npx cypress run",
    "e2e:run:testContent": "APPLITOOLS_IS_DISABLED=true npx cypress run --env MATCH_CONTENT_STATE=true",
    "e2e:run:update": "APPLITOOLS_IS_DISABLED=true npm run e2e:run --env CYPRESS_CONFIG=update,updateSnapshots=true",
    "e2e:app": "cd e2e/test-env && npm run build && npm start",
    "e2e:app:run": "cd e2e/test-env && npm start",
    "e2e:app:dev": "cd e2e/test-env && npm run dev",
    "e2e:app:wait": "wait-on http://localhost:3002/ --timeout 220000 --interval 2000",
    "test": "lerna run --stream test",
    "lint": "lerna run --stream lint",
    "build:test": "npm run build && npm run build --prefix e2e/test-env",
    "generatePlugin": "node scripts/generatePlugin",
    "migrateFixtures": "cd scripts && ts-node --compiler-options={\\\"module\\\":\\\"commonJS\\\"} migrateSchema",
    "toPlainText": "cd scripts && ts-node --compiler-options={\\\"module\\\":\\\"commonJS\\\"} toPlainText",
    "link": "lerna link"
  },
  "ultra": {
    "concurrent": [
      "lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "node scripts/preCommit"
    }
  },
  "devDependencies": {
    "@actions/github": "^2.1.1",
